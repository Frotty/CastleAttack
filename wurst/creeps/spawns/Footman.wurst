package Footman
import Creep
import UnitObjEditing
import GameConstants
import ObjectIds

public constant FOOTMAN_ID = OBJ_ED_PREFIX_UNIT + '0ftv'

public class CreepFootman extends Creep
	
	construct(vec3 pos, player owner, vec2 target)
		super(pos, FOOTMAN_ID, owner, target)
		


@compiletime function generateFootmen()
	let def = new UnitDefinition( FOOTMAN_ID, 'hfoo' )
	def.setHitPointsMaximumBase(100)
	def.setName("Footman")
